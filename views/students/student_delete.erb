<h3>Delete Student</h3>

  <p><h4>Choose a student to delete:</h4></p>
    <% Student.all.each do |student| %>
        <input type="radio" name="id" value="<%= student.id %>">
        <%= student.date%> <%= student.name%>
    <% end %>
    <p><input type="submit"></p>
    
<p><a href = "/home">Return To Main Menu</a><p>